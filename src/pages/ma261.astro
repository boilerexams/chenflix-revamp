<script>
  const button = document.getElementById('reverseButton')
  const icon = document.getElementById('icon')
  const container = document.getElementById('classEventContainer')
  let isSpinning = false

  if (button && container && icon) {
    button.addEventListener('click', function () {
      Array.from(container.children)
        .reverse()
        .forEach((child) => container.appendChild(child))

      // Add or remove the spin animation
      if (isSpinning) {
        icon.classList.remove('fa-spin')
        icon.classList.add('fa-spin-reverse')
      }

      isSpinning = !isSpinning

      setTimeout(function () {
        icon.classList.remove('fa-spin')
        icon.classList.remove('fa-spin-reverse')
      }, 1050)
    })
  }
</script>

<style>
  .fa-spin-reverse {
    animation-direction: reverse;
  }
</style>

